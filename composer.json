{
  "repositories": [],
  "require": {
    "automattic/jetpack-autoloader": "^5.0",
    "guzzlehttp/guzzle": "^7.8",
    "guzzlehttp/promises": "^2.0",
    "mehedimi/wp-query-builder-ext": "^0.3.0",
    "monolog/monolog": "^3.0",
    "openai-php/client": "^0.10.3",
    "php-jsonpatch/php-jsonpatch": "^4.1",
    "surecart/wordpress-sdk": "^1.0",
    "woocommerce/action-scheduler": "^3.9",
    "x-wp/asset-loader": "^1.1",
    "x-wp/di": "^2.0.0-alpha.5",
    "x-wp/helper-classes": "^1.19",
    "yethee/tiktoken": "^0.7.0"
  },
  "require-dev": {
    "oblak/wordpress-coding-standard": "^1",
    "php-stubs/wordpress-stubs": "^6.5",
    "php-stubs/wp-cli-stubs": "^2.11",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-deprecation-rules": "^1.1",
    "swissspidy/phpstan-no-private": "^0.2.0",
    "symfony/var-dumper": "^6.4",
    "szepeviktor/phpstan-wordpress": "^1.3",
    "wpsyntex/polylang-stubs": "^3.6"
  },
  "minimum-stability": "alpha",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "EPIC_WP\\Polylang_Automatic_AI_Translation\\": "src/Legacy/",
      "PLLAT\\": "src/Modules/",
      "PLLAT\\Common\\": "src/Common/"
    },
    "classmap": [
      "src/App.php"
    ],
    "files": [
      "src/Common/Functions/pllat-compat-fns.php"
    ],
    "exclude-from-classmap": []
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "automattic/jetpack-autoloader": true
    },
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true
  },
  "extra": {
    "release": {
      "name": "Polylang Automatic Translation with AI (Free)",
      "slug": "polylang-ai-automatic-translation"
    }
  }
}
